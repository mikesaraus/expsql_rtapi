{
  "name": "payment_system",
  "version": "0.0.1",
  "productName": "Payment System",
  "description": "Realtime API for Payment System",
  "displayName": "PSQL-Express Realtime API",
  "private": true,
  "main": "app.js",
  "scripts": {
    "setup": "node app.js --gensql database.sql --genenv",
    "dev": "set DEBUG=express:* && nodemon -V --delay 2500ms --watch '*.*' --watch .env --ext 'js,json' app.js --log",
    "dev-nolog": "set DEBUG=express:* && nodemon -V --delay 2500ms --watch '*.*' --watch .env --ext 'js,json' app.js",
    "start": "set NODE_ENV=production && node app.js --log"
  },
  "keywords": [
    "NodeJS",
    "ExpressJS",
    "PostgreSQL"
  ],
