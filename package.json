    "name": "mikesaraus"
  },
  "license": "0BSD",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "helmet": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash.get": "^4.4.2",
    "morgan": "^1.10.0",
    "pdfkit": "^0.13.0",
    "pg": "^8.7.1",
    "socket.io": "^4.4.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
