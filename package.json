{
  "name": "realtime_api",
  "version": "0.0.2",
  "description": "Express and PostgreSQL Realtime API",
  "private": true,
  "main": "app.js",
  "scripts": {
    "setup": "node app.js --gensql database.sql --genenv",
    "dev": "DEBUG=express:* nodemon -V --delay 2500ms --watch '*.*' --watch .env --ext 'js,json' app.js",
    "start": "NODE_ENV=production node app.js 2>&1 >> ./log/console-\"$(date '+%Y%m%d-%H%M%S')\".log"
  },
  "keywords": [],
  "author": {
    "name": "mikesaraus"
  },
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "helmet": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "rotating-file-stream": "^3.0.2",
    "socket.io": "^4.4.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
