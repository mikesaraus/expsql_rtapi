{
  "name": "payment_system",
  "version": "0.0.2",
  "description": "Realtime API for Payment System",
  "private": true,
  "main": "app.js",
  "scripts": {
    "setup": "node app.js --gensql database.sql --genenv",
    "dev": "DEBUG=express:* nodemon -V --delay 2500ms --watch '*.*' --watch .env --ext 'js,json' app.js",
